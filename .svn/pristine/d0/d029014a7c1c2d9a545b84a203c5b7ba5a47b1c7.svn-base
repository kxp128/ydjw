package com.ehootu.notice.entity;

import java.io.Serializable;
import java.util.Date;
import java.util.List;


/**
 * 通知消息
 * 
 * @author zhangyong
 * @email zhangyong@ehootu.com
 * @date 2018-01-29 10:17:49
 */
public class NoticeEntity implements Serializable {
	private static final long serialVersionUID = 1L;
	
	//
	private String id;
	//标题
	private String title;
	//创建人
	private String creater;
	//内容
	private String content;
	//发布时间
	private Date publishTime;
	//创建时间
	private Date createTime;
	//修改时间
	private Date updateTime;
	//
	private String deptId;
	//删除标记
	private Integer deleteFlag;
	//删除标记时间
	private Date deleteFlagTime;
	//排序
	private Integer num;
	//
	private Integer version;
	//是否已经同步，默认为0-未同步；1-已同步
	private Integer isSync;

	//警察ID列表
	private List<String> policeIdList;

	//判断已读未读
	private Integer seeFlag;

	/**
	 * 设置：
	 */
	public void setId(String id) {
		this.id = id;
	}
	/**
	 * 获取：
	 */
	public String getId() {
		return id;
	}
	/**
	 * 设置：标题
	 */
	public void setTitle(String title) {
		this.title = title;
	}
	/**
	 * 获取：标题
	 */
	public String getTitle() {
		return title;
	}
	/**
	 * 设置：创建人
	 */
	public void setCreater(String creater) {
		this.creater = creater;
	}
	/**
	 * 获取：创建人
	 */
	public String getCreater() {
		return creater;
	}
	/**
	 * 设置：内容
	 */
	public void setContent(String content) {
		this.content = content;
	}
	/**
	 * 获取：内容
	 */
	public String getContent() {
		return content;
	}
	/**
	 * 设置：发布时间
	 */
	public void setPublishTime(Date publishTime) {
		this.publishTime = publishTime;
	}
	/**
	 * 获取：发布时间
	 */
	public Date getPublishTime() {
		return publishTime;
	}
	/**
	 * 设置：创建时间
	 */
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	/**
	 * 获取：创建时间
	 */
	public Date getCreateTime() {
		return createTime;
	}
	/**
	 * 设置：修改时间
	 */
	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}
	/**
	 * 获取：修改时间
	 */
	public Date getUpdateTime() {
		return updateTime;
	}
	/**
	 * 设置：
	 */
	public void setDeptId(String deptId) {
		this.deptId = deptId;
	}
	/**
	 * 获取：
	 */
	public String getDeptId() {
		return deptId;
	}
	/**
	 * 设置：删除标记
	 */
	public void setDeleteFlag(Integer deleteFlag) {
		this.deleteFlag = deleteFlag;
	}
	/**
	 * 获取：删除标记
	 */
	public Integer getDeleteFlag() {
		return deleteFlag;
	}
	/**
	 * 设置：删除标记时间
	 */
	public void setDeleteFlagTime(Date deleteFlagTime) {
		this.deleteFlagTime = deleteFlagTime;
	}
	/**
	 * 获取：删除标记时间
	 */
	public Date getDeleteFlagTime() {
		return deleteFlagTime;
	}
	/**
	 * 设置：排序
	 */
	public void setNum(Integer num) {
		this.num = num;
	}
	/**
	 * 获取：排序
	 */
	public Integer getNum() {
		return num;
	}
	/**
	 * 设置：
	 */
	public void setVersion(Integer version) {
		this.version = version;
	}
	/**
	 * 获取：
	 */
	public Integer getVersion() {
		return version;
	}
	/**
	 * 设置：是否已经同步，默认为0-未同步；1-已同步
	 */
	public void setIsSync(Integer isSync) {
		this.isSync = isSync;
	}
	/**
	 * 获取：是否已经同步，默认为0-未同步；1-已同步
	 */
	public Integer getIsSync() {
		return isSync;
	}

	public List<String> getPoliceIdList() {
		return policeIdList;
	}

	public void setPoliceIdList(List<String> policeIdList) {
		this.policeIdList = policeIdList;
	}

	public Integer getSeeFlag() {
		return seeFlag;
	}

	public void setSeeFlag(Integer seeFlag) {
		this.seeFlag = seeFlag;
	}

	@Override
	public String toString() {
		return "NoticeEntity{" +
				"id='" + id + '\'' +
				", title='" + title + '\'' +
				", creater='" + creater + '\'' +
				", content='" + content + '\'' +
				", publishTime=" + publishTime +
				", createTime=" + createTime +
				", updateTime=" + updateTime +
				", deptId='" + deptId + '\'' +
				", deleteFlag=" + deleteFlag +
				", deleteFlagTime=" + deleteFlagTime +
				", num=" + num +
				", version=" + version +
				", isSync=" + isSync +
				", policeIdList=" + policeIdList +
				'}';
	}
}
