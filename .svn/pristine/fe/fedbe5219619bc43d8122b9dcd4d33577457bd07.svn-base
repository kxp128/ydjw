	/**
	 * 拼接七段地址
	 * @return
	 */
	public String getAddress() {
		
		StringBuffer result = new StringBuffer();
		// 行政区划名称
		result.append(xzqhmc);

		// 街路巷名称
		result.append(jlxmc);

		// 门牌号名称
		result.append(mphmc);
		// 门牌号后缀
		result.append(mphhz);

		// 楼牌号名称
		result.append(lphmc);
		// 楼牌号后缀
		result.append(lphhz); 
		
		//单元号名称
		result.append(dyhmc);
		//单元号后缀
		result.append(dyhhz);
		//层号名称
		result.append(chmc);
		//层号后缀
		result.append(chhz);
		
		//户号名称
		result.append(hhmc);
		//户号后缀
		result.append(hhhz);
		
		return result.toString() ;
	}

